<script>
    window.onload = function() {
        var arrheight = [];

        function arrHeight() {
            var items = document.getElementsByClassName('right-list');
            for (var i = 0; i < items.length; i++) {
                var scrolltop = items[i].offsetTop;
                arrheight.push(scrolltop);
            }
        }

        function noactive() {
            var item = document.getElementsByClassName('list')
            for (var i = 0; i < item.length; i++) {
                item[i].setAttribute('class', 'list');
            }
        }

		// 左侧根据右侧当前滚动到的类别添加active
        function scrollactive() {
            var scrolltop = document.getElementById('right-hook').scrollTop;
            for (var i = 0; i < arrheight.length; i++) {
                if (scrolltop > arrheight[i] && scrolltop < arrheight[i + 1]) {
                    noactive();
                    document.getElementsByClassName('list')[i].setAttribute('class', 'list list-active');
                }
            }
        }
		//左侧点击添加样式
        function clickactve() {
            // var list = document.getElementsByClassName('list');
			var list =$(".list");
            for (var i = 0; i < list.length; i++) {
                list[i].onclick = function() {
                    noactive();
                    // list[i].setAttribute('class','list list-active');
					list.eq(i).addClass("list list-active");
                }
            }
        }
		
		function scrollTitle(){
			var scrolltop =document.getElementById('right-hook').scrollTop;
			var titleHeight=$(".top-title").outerHeight();
			var rightList=$(".right-list");
			for(var i=0;i<rightList.length;i++){
				var offsetTop=rightList[i].offsetTop;
				var offsetTop2=rightList[i+1].offsetTop;
				if(scrolltop>=offsetTop&&scrolltop<offsetTop2-titleHeight){
					$(".top-title").css("top","0");
					$(".top-title .txt").text(rightList.eq(i).find(".title .txt").text());
					break;
				}else if(scrolltop>=offsetTop2-titleHeight&&scrolltop<offsetTop2){
					$(".top-title").css("top",-(titleHeight+scrolltop-offsetTop2)+"px");
					$(".top-title .txt").text(rightList.eq(i).find(".title .txt").text());
					break;
				}else if(scrolltop==offsetTop2){
					$(".top-title").css("top","0");
					$(".top-title .txt").text(rightList.eq(i+1).find(".title .txt").text());
					break;
				}
			}
		}
        document.getElementById('right-hook').onscroll = function() {
            scrollactive();
			scrollTitle();
        };
        arrHeight();
        scrollactive();
        clickactve();
    }
	$(document).on('click', '.list', function(e) {
		$this=e.target;
		$(this).siblings(".list").removeClass("list-active");
		$(this).addClass("list-active");
	})
</script>
<script type="text/javascript">

    $(function () {
        //菜品详情
        var ph = $(".pop-shop").height();
        var pop = ph / 2 * (-1);
        $(".pop-shop").css({
            'margin-top': pop + 'px'
        });
        $(".combo ul li").click(function () {
            $(this).addClass("hover").siblings().removeClass("hover")
        });
        $(".cart-list").click(function () {
            $(".pop-bg").show().css({"z-index":"1004"});
            $(".pop-shop").show();
        });
        $(".close-btn").click(function () {
            $(".pop-bg").hide().css({"z-index":"1000"});
            $(".pop-shop").hide();
        });


        //左侧菜单
        var wH = $(window).outerHeight();
        var hH = $(".head").outerHeight();
        var tH = $(".top").outerHeight();
		var sH=$(".sell-header").outerHeight();
        var fH = $(".f-wrap").outerHeight();
        $(".container,.left,.left .element,.right,.right .element").css({"height":wH-sH-fH+2+"px"});

        $(".cart-list .ones").hide();


        //加
        $(".add").click(function () {
            $(this).parent(".nub").find(".ones").css({"display":"inline-block"});
            var tmp = parseInt($(this).parent(".nub").find("em").text());
            tmp += 1;
            $(this).parent(".nub").find("em").text(tmp);
            $(".shoppingBtn").addClass("act");
            $(".shoppingBtn i").text(tmp).addClass("act");
			$(".check-out-btn").addClass("btn-active");
            //$(".f-wrap").show();
            //$(".container,.left,.left .element,.right,.right .element").css({"height":wH-hH-tH-fH+2+"px"});

            $(".shoppingBtn").click(function () {
                $(".pop-bg").show();
                $(".shop").show();
            });
            $(".pop-bg").click(function () {
                $(".pop-bg").hide();
                $(".shop").hide();
                $(".pop-shop").hide();
            });

            return false;
        });

        //减
        $(".plus").click(function () {
            var tmp = parseInt($(this).next().text());
            tmp -= 1;
            $(this).next().text(tmp);
            $(".shoppingBtn i").text(tmp);
            if ($(this).next().text() == "0") {
                $(this).parent(".ones").hide();
            }
            if ($(".shoppingBtn i").text() == "0") {
                $(".shoppingBtn").removeClass("act");
                $(".shoppingBtn i").text(tmp).removeClass("act");
				$(".check-out-btn").removeClass("btn-active");
                //$(".f-wrap").hide();
            }
            //$(".container,.left,.left .element,.right,.right .element").css({"height":wH-hH-tH+2+"px"});

            $(".shoppingBtn").unbind("click");
            return false;
        });
        if ($(".cart-zhi").text() > "0") {
            $(".menu-footer .ones").css({"display":"inline-block"});
        }
    });

    //添加菜品动画效果
    $(".add").on("click", addProduct);
    function addProduct(event) {
        var x = event.pageX - $(document).scrollLeft();
        var y = event.pageY - $(document).scrollTop();
        var t = $(document).scrollTop();
        var l = $(document).scrollLeft();
        var offset = $('#end').offset(),
            flyer = $("<lable class='pointer'></label>");
        flyer.fly({
            start: {
                left: x,
                top: y
            },
            end: {
                left: offset.left - l,
                top: offset.top - t,
                width: 10,
                height: 10
            }
        });
    }
</script>
