.magic-checkbox {
    position: absolute;
    display: none;
}

    .magic-checkbox[disabled] {
        cursor: not-allowed;
    }

    .magic-checkbox + label {
        position: relative;
        display: block;
        cursor: pointer;
    }

        .magic-checkbox + label:before {
            position: absolute;
            top: 0;
            left: 0;
            display: inline-block;
            width: 0.5rem;
            height: 0.5rem;
            content: '';
            /*border: .1rem solid #c0c0c0;*/
            background: #fff;
            border-radius: 1.1rem;
        }
				.card-radio .magic-checkbox + label:before {
							width: 2rem;
          	  height: 2rem;
           }
        .magic-checkbox + label:after {
            position: absolute;
            display: none;
            content: '';
        }

    .magic-checkbox:checked + label:after {
        display: block;
    }

    .magic-checkbox + label:after {
        top: 0.08rem;
        left: 0.15rem;
        box-sizing: border-box;
        transform: rotate(45deg);
        border: .1rem solid #fff;
        border-top: 0;
        border-left: 0;
    }
.card-radio .magic-checkbox + label:after {
	    top: 0.2rem;
    left: 0.7rem;
	    width: .7rem;
    height: 1.3rem;
        border: .3rem solid #fff;
            border-top: 0;
    border-left: 0;
}
    .magic-checkbox:checked + label:before {
        border: #d6a465;
        background: #d6a465;
    }

