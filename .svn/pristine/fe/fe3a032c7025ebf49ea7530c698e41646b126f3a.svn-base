.magic-radio,.magic-checkbox { position: absolute; display: none; }
.magic-radio + label,.magic-checkbox + label { position: relative; display: block; padding-left: .7rem; cursor: pointer; vertical-align: middle; line-height: .55rem }
.magic-radio + label:before,.magic-checkbox + label:before { position: absolute;top: 0;left: 0;display: inline-block;width: .5rem;height: .5rem;content: '';border: .02rem solid #c0c0c0;}
.magic-radio + label:after,.magic-checkbox + label:after { position: absolute; display: none; content: ''; }
.magic-radio:checked + label:after,.magic-checkbox:checked + label:after {  display: block; }
.magic-radio + label:before { border-radius: 50%; }
.inpBox .magic-radio + label:before{
	top: 0.05rem;
    left: 0;
	    width: .4rem;
    height: .4rem;
}
	
.inpBox .magic-radio + label:after{
	    top: .15rem;
    left: .095rem;
		width: .25rem;
		height: .25rem;
}
.m25.agreementBox{margin: .3rem;}
.agreementBox .magic-checkbox + label:before{
	    width: 0.4rem;
    height: 0.4rem;
	    border: .02rem solid #c0c0c0;
			border-radius:0;
	background:#fff;
	-moz-box-shadow:-.02rem -.02rem .08rem #cecece inset;             /* For Firefox3.6+ */
	-webkit-box-shadow:-.02rem -.02rem .08rem #cecece inset;          /* For Chrome5+, Safari5+ */
	box-shadow:-.02rem -.02rem .08rem #cecece inset;                  /* For Latest Opera */
}
.agreementBox .magic-checkbox:checked + label:before{
	    width: 0.4rem;
    height: 0.4rem;
	    border: .02rem solid #c0c0c0;
			border-radius:0;
	background:#fff;
	-moz-box-shadow:-.02rem -.02rem .08rem #cecece inset;             /* For Firefox3.6+ */
	-webkit-box-shadow:-.02rem -.02rem .08rem #cecece inset;          /* For Chrome5+, Safari5+ */
	box-shadow:-.02rem -.02rem .08rem #cecece inset;                  /* For Latest Opera */
}
.agreementBox .magic-checkbox + label:after{
	border:none;
	transform:none;
	top:.02rem;
	left:.02rem;
	background: url("../../images/content/red-checked@2x.png") center center no-repeat;
	background-size: .45rem;
	width:.4rem;
	height:.4rem;
}
.agreementBox .btn{
	background:#959595;
}
.magic-radio + label:after { top: .06rem; left: .06rem; width: .41rem; height: .41rem; border-radius: 50%; background: #d4374c;  }
.magic-radio:checked + label:before { border: .02rem solid #d4374c; }
.magic-checkbox + label:before { border-radius: .1rem; }
.magic-checkbox + label:after { top: .03rem; left: .15rem; box-sizing: border-box; width: .2rem; height: .3rem; transform: rotate(45deg); border-width: .05rem; border-style: solid; border-color: #fff; border-top: 0; border-left: 0; }
.magic-checkbox:checked + label:before { border: #000; background: #000; }
