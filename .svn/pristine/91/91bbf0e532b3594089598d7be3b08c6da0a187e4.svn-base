.rating-wrapper {
	padding: 0 1rem;
}

.rating-wrapper .rating-item {
	padding: 1rem 0;
	border-bottom: 1px solid #e4e5ea;
}

.rating-wrapper .rating-item:last-child {
	border: none;
}

.user-wrapper {
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.user-wrapper .avatar {
	width: 3.2143rem;
	height: 3.2143rem;
	margin-right: 0.7143rem;
	border-radius: 50%;
}

.user-wrapper .user-info-text {
	flex: 1;
}

.user-wrapper .user-info-text .name {
	line-height: 1.5rem;
	font-size: 1rem;
	color: #111;
}

.user-wrapper .time {
	font-size: 0.8571rem;
	color: #888;
}

.rating-wrapper .comment {
	padding: .8rem 0;
	line-height: 1.4286rem;
	font-size: 1rem;
	color: #444;
}

.rating-wrapper .pics {
	display: flex;
	justify-content: space-between;
}

.rating-wrapper .pics .img-item {
	width: 23%;
}

.rating-wrapper .pics img {
	width: 100%;
	height: 5.6429rem;
	border-radius: 5px;
}

/* 五角星评分 */
.star {
	font-size: 0;
}

.star .star-item {
	display: inline-block;
	background-repeat: no-repeat;
	width: 0.7143rem;
	height: 0.7143rem;
	margin-right: 0.2143rem;
	background-size: 0.7143rem 0.7143rem;
}

.star .star-item:last-child {
	margin-right: 0;
}

.star .star-item.on {
	background-image: url("../../images/interactive/star24_on@2x.png");
}

.star .star-item.half {
	background-image: url("../../images/interactive/star24_half@2x.png");
}

.star .star-item.off {
	background-image: url("../../images/interactive/star24_off@2x.png");
}

@media (-webkit-min-device-pixel-ratio:3),
(min-device-pixel-ratio:3) {
	.star .star-item.on {
		background-image: url("../../images/interactive/star24_on@3x.png");
	}

	.star .star-item.half {
		background-image: url("../../images/interactive/star24_half@3x.png");
	}

	.star .star-item.off {
		background-image: url("../../images/interactive/star24_off@3x.png");
	}
}

/*  */
.detail-btn {
	display: flex;
	justify-content: space-around;
	align-items: center;
	position: fixed;
	bottom: .8rem;
	left: 50%;
	transform: translateX(-50%);
	-webkit-transform: translateX(-50%);
	width: 90%;
	/* padding:1rem 0; */
	line-height: 3.5rem;
	font-size: 1.1429rem;
	color: #fff;
	background: -webkit-linear-gradient(left, #edb884, #c78a51);
	background: -o-linear-gradient(right, #edb884, #c78a51);
	background: -moz-linear-gradient(right, #edb884, #c78a51);
	background: linear-gradient(to right, #edb884, #c78a51);
	border-radius: 5rem;
	z-index: 9999;
}

.detail-btn a {
	position: relative;
	flex: 1;
	text-align: center;
	display: block;
	color: #fff;
}

.detail-btn a:first-child::after {
	width: .1rem;
	height: 1rem;
	content: "";
	background-color: #fff;
	position: absolute;
	right: 0;
	top: 50%;
	transform: translateY(-50%);
	-webkit-transform: translateY(-50%);
	z-index: 2;
}

.detail-btn i {
	display: inline-block;
	vertical-align: middle;
	width: 2rem;
	height: 2rem;
	position: relative;
	top: -.1rem;
}

.detail-btn .tel {
	background: url(../../images/interactive/yuyue@2x.png) center center no-repeat;
	background-size: 1.4286rem;

}

.detail-btn .order {
	background: url(../../images/interactive/diancan@2x.png) center center no-repeat;
	background-size: 1.4286rem;
}

@media (-webkit-min-device-pixel-ratio:3),
(min-device-pixel-ratio:3) {
	.detail-btn .tel {
		background: url(../../images/interactive/yuyue@3x.png) center center no-repeat;
		background-size: 1.4286rem;
	}

	.detail-btn .order {
		background: url(../../images/interactive/diancan@3x.png) center center no-repeat;
		background-size: 1.4286rem;
	}
}

/* 点餐页 */
.sell-header {
	position: fixed;
	left: 0;
	top: 0;
	z-index: 1;
	width: 100%;
}

.sell-header img.sellBg {
	width: 100%;
	height: 10.5rem;
}

.sell-header .mark-info {
	position: absolute;
	right: 1rem;
	bottom: 1rem;
	padding: 0.3rem .5rem;
	color: #fff;
	/* height:1.78rem; */
	font-family: "微软雅黑", "Open Sans", "Helvetica Neue", Helvetica, Arial, sans-serif;
	font-size: 1rem;
	font-weight: 300;
	background: rgba(0, 0, 0, .7);
	border-radius: 20px;
}

.left-menu::-webkit-scrollbar {
	width: 0;
}

.main {
	overflow: hidden;
}

.left-menu {
	width: 25%;
	float: left;
	background-color: #eee;
	position: relative;
	overflow-y: scroll;
}

.left-menu ul li {
	cursor: pointer;
	border-bottom: 1px solid #d0d0d0;
	font-size: 1.1rem;
	text-align: center;
	color: #999;
	padding: 1rem .5rem
}

.left-menu ul li:last-child {
	border-bottom: 0
}

.left-menu ul li.act {
	color: #fff !important;
	background-color: #999;
}

.right-con {
	display: none;
	width: 75%;
	float: left;
	overflow-y: scroll;
	background: #fff;
	position: relative;
	height: 500px;
}

.con .con-active {
	display: block;
}

