}

/* 首页 */
.bg-dark {
	background: #1f1f1f;
}

.bg-dark * {
	box-sizing: border-box;
}

.catalog-header {
	position: relative;
	width: 100%;
	height: 26.7857rem;
	color: #b38c63;
	background: url(../../images/interactive/banner.jpg) center center no-repeat;
	background-size: 100% 100%;
}

.catalog-header img {
	width: 100%;
	height: 26.7857rem;
}

.catalog-header .header-dec {
	position: absolute;
	bottom: 2.13rem;
	width: 100%;
	text-align: center;
}

.catalog-header .header-dec h1 {
	font-size: 1.2857rem;
	margin-bottom: -.3rem;
}

.catalog-header .header-dec p {
	display: block;
	/* padding:1rem 0; */
}

.catalog-header .header-dec .info {
	/* padding:2.1429rem 0; */
	line-height: 2.3rem;
	font-size: 1rem;
}

.catalog-header .header-dec .tag {
	display: inline-block;
	padding: 0.15rem .3rem;
	margin: .6rem 0.1786rem;
	font-size: 0.7143rem;
	border: .1rem solid #b38c63;
	border-radius: .2rem;
}

.catalog-header .line {
	display: inline-block;
	width: 100%;
	height: .5rem;
	background: url(../../images/interactive/line@2x.png) center center no-repeat;
	background-size: 7.15rem;
}

.catalog-wrapper {
	padding: 0 1rem 10.25rem;
}

.catalog-wrapper .dining-title {
	font-family: "NotoSansSC";
	font-size: 1.2857rem;
	color: #c78c54;
}

.catalog-wrapper .dining-title span {
	display: block;
	background: url(../../images/interactive/xiaonanshan-title@2x.png) center center no-repeat;
	background-size: 12.7143rem;
	width: 100%;
	height: 3.2143rem;
	margin-top: 1rem;
}

.catalog-wrapper li:nth-child(2) .dining-title span {
	background: url(../../images/interactive/taoyuan-title@2x.png) center center no-repeat;
	background-size: 12.7143rem;
}

.catalog-wrapper li:nth-child(3) .dining-title span {
	background: url(../../images/interactive/chashi-title@2x.png) center center no-repeat;
	background-size: 12.7143rem;
}

.catalog-wrapper li:nth-child(4) .dining-title span {
	background: url(../../images/interactive/qingba-title@2x.png) center center no-repeat;
	background-size: 12.7143rem;
}

@media (-webkit-min-device-pixel-ratio:3),
(min-device-pixel-ratio:3) {
	.catalog-wrapper .dining-title span {
		background: url(../../images/interactive/xiaonanshan-title@3x.png) center center no-repeat;
		background-size: 12.7143rem;
	}

	.catalog-wrapper li:nth-child(2) .dining-title span {
		background: url(../../images/interactive/taoyuan-title@3x.png) center center no-repeat;
		background-size: 12.7143rem;
	}

	.catalog-wrapper li:nth-child(3) .dining-title span {
		background: url(../../images/interactive/chashi-title@3x.png) center center no-repeat;
		background-size: 12.7143rem;
	}

	.catalog-wrapper li:nth-child(4) .dining-title span {
		background: url(../../images/interactive/qingba-title@3x.png) center center no-repeat;
		background-size: 12.7143rem;
	}
}

.catalog-wrapper .dining-dec {
	background: #fff;
	border-radius: 6px;
}

.catalog-wrapper .dining-dec a {
	display: block;
}

.catalog-wrapper .dining-dec img {
	width: 100%;
	height: 12.3571rem;
}

.catalog-wrapper .content {
	padding: .8rem 1rem;
}

.catalog-wrapper .content .name {
	display: flex;
	align-items: center;
	height: 2.14rem;
	overflow: auto;
}

.catalog-wrapper .content .name h2 {
	flex: 0 0 auto;
	font-family: "SourceHanSansSC-Light", "Source-Han-Light", "Source Han Sans CN", "SourceHanSansCN-Light", "webfontSourceHanSansSC";
	font-size: 1.2857rem;
	font-weight: 600;
	/* min-width:8rem; */
}

.catalog-wrapper .content .name .tag {
	display: inline-block;
	/* min-width:100%; */
	height: 1.1rem;
	line-height: 1.1rem;
	padding: 0 .3rem;
	margin-left: .5rem;
	white-space: nowrap;
	font-size: 0.6429rem;
	color: #b38c63;
	border: .1rem solid #b38c63;
	border-radius: .2rem;
}

.catalog-wrapper .content .more {
	display: flex;
	justify-content: space-between;
	align-items: center;
	line-height: 2.65rem;
}

.catalog-wrapper .content .more .time {
	/* flex:8; */
}

.catalog-wrapper .content .more .time span {
	display: inline-block;
	background: -webkit-linear-gradient(left, #c09a75, #b28c67);
	background: -o-linear-gradient(right, #c09a75, #b28c67);
	background: -moz-linear-gradient(right, #c09a75, #b28c67);
	background: linear-gradient(to right, #c09a75, #b28c67);
	height: 1.7143rem;
	line-height: 1.7143rem;
	padding: 0 .5rem;
	margin-right: .35rem;
	font-size: 0.8571rem;
	/* font-weight: 400; */
	color: #fff;
	border-radius: .2rem;
}

.catalog-wrapper .content .more .tel {
	width: 4rem;
	font-size: 1rem;
	color: #b38c63;
}

.catalog-wrapper .content .more .tel i {
	display: inline-block;
	vertical-align: middle;
	background: url(../../images/interactive/tel@2x.png) center center no-repeat;
	background-size: 1.2143rem;
	width: 1.2143rem;
	height: 1.7143rem;
}


@media (-webkit-min-device-pixel-ratio:3),
(min-device-pixel-ratio:3) {
	.catalog-wrapper .content .more .tel i {
		background: url(../../images/interactive/tel@3x.png) center center no-repeat;
		background-size: 1.2143rem;
	}
}

/* 详情页 */
.detail-wrapper * {
	box-sizing: border-box;
}

.detail-header.swiper-container {
	/* padding:1rem; */
	padding: 1rem 1rem 1rem 1rem;
	width: 100%;
	margin: 0;
}

.detail-header .swiper-wrapper {
	/* padding:0 1rem; */
	width: 100%;
	height: 11.7857rem;
	box-sizing: border-box;
}

.detail-header .swiper-slide {
	/* margin-left:1rem; */
}

.detail-header img {
	/* width: 97%; */
	width: 100%;
	height: 100%;
	box-shadow: 1px 1px 6px #D0D0D0;
	/* box-shadow: -1px 0px 3px #ccc,   
	 0px -1px 3px #D0D0D0, 
	 1px 0px 3px #D0D0D0, 
	 0px 1px 3px #ccc; */
	border-radius: 6px;
}

.content-introduce {
	padding: 0 1rem;
}

.content-introduce .name {
	display: flex;
	align-items: center;
	height: 2.14rem;
	overflow: auto;
}

.content-introduce .name h2 {
	flex: 0 0 auto;
	font-family: "SourceHanSansSC-Light", "Source-Han-Light", "Source Han Sans CN", "SourceHanSansCN-Light", "webfontSourceHanSansSC";
	font-size: 1.2857rem;
	font-weight: 600;
	/* min-width:8rem; */
}

.content-introduce .name .tag {
	display: inline-block;
	/* min-width:100%; */
	height: 1.35rem;
	line-height: 1.35rem;
	padding: 0 .4rem;
	margin-left: .5rem;
	white-space: nowrap;
	font-size: 0.7857rem;
	color: #fff;
	background: -webkit-linear-gradient(left, #eeb985, #c98e56);
	background: -o-linear-gradient(right, #eeb985, #c98e56);
	background: -moz-linear-gradient(right, #eeb985, #c98e56);
	background: linear-gradient(to right, #eeb985, #c98e56);
	border-radius: .2rem;
}

.content-introduce .info {
	padding: .5rem 0;
	line-height: 1.2rem;
	color: #888;
	font-size: 0.8571rem;
}

.content-introduce .time span {
	display: inline-block;
	height: 1.35rem;
	line-height: 1.35rem;
	padding: 0 .5rem;
	margin-right: .35rem;
	font-size: 0.7857rem;
	/* font-weight: 400; */
	color: #c78b52;
	border: .1rem solid #c78b52;
	border-radius: .2rem;
}

.item-container {
	/* padding: 1rem 0; */
	margin-top: 1rem;
}

.item-container .item-title {
	display: flex;
	justify-content: space-between;
	align-items: center;
	padding: 1rem 1rem;
	/* margin-bottom: 1rem; */
	font-size: 1.1429rem;
	font-weight: 600;
}

.item-container .item-title .more {
	display: flex;
	font-size: 0.8571rem;
	font-weight: 400;
	color: #888;
}

.item-container .item-title .more i {
	vertical-align: middle;
}

.item-container .item-content li {
	position: relative;
	padding: 0 1rem;
	margin-bottom: 1.4286rem;
	width: 100%;
	display: flex;
	background: #fff;
}

.item-container .item-content li:last-child {
	margin-bottom: 0;
}

.item-container .item-content img {
	width: 6rem;
	height: 6rem;
	margin-right: 0.7143rem;
	border-radius: 5px;
}

.item-container .item-content .detail {
	flex: 1;
	/* width: 0; */
	overflow: hidden;
	display: flex;
	flex-direction: column;
	justify-content: space-between;
}

.item-container .item-content h3 {
	font-size: 1.1429rem;
	font-weight: 600;
}

.item-container .item-content .intro {
	line-height: 1.4rem;
	font-size: 0.8571rem;
	color: #888;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.item-container .item-content .tag {
	display: inline-block;
	height: 1.2857rem;
	line-height: 1.2857rem;
	padding: 0 .4rem;
	border-radius: .2rem;
	margin-right: 0.7143rem;
	font-size: 0.6429rem;
}

.item-container .item-content .tag.red-tag {
	color: #ef5e49;
	background: #fee9e4;
}

.item-container .item-content .tag.base-tag {
	color: #888;
	border: .1rem solid #d6d7dc;
}

.item-container .item-content .new-price {
	color: #ec5e48;
	font-size: 1.2857rem;
}

.item-container .item-content .new-price i {
	font-style: normal;
	font-size: 0.7857rem;
}

.item-container .item-content .old-price {
	color: #888;
	font-size: 0.7857rem;
	text-decoration: line-through;
}

.item-container .item-content .old-price i {
	font-style: normal;

}

.recommend-content,
.recommend-content * {
	box-sizing: border-box;
}

.recommend-content.swiper-container1 {
	/* padding:1rem; */
	/* padding:1rem 1rem 1rem 1rem; */
