	margin: 0;
}

.recommend-content .swiper-wrapper {
	/* padding:0 1rem; */
	/* padding-left:1rem; */
	width: 100%;
	/* height: 11.7857rem; */
	box-sizing: border-box;
}

.recommend-content .swiper-slide:first-child {
	/* margin-left:1rem; */
}

.recommend-content .swiper-slide {}

.recommend-content img {
	/* width:28%; */
	width: 100%;
	/* height: 100%; */
	height: 5.3214rem;
	box-shadow: 1px 1px 6px #D0D0D0;
	/* box-shadow: -1px 0px 3px #ccc,   
			 0px -1px 3px #D0D0D0, 
